{
  "rules": {
    "require-project-name-field": 0,
    "services-alphabetical-order": 0,
    "service-keys-order": 2,
    "service-image-require-explicit-tag": 2,
    "no-unbound-port-interfaces": 2,
    "require-quotes-in-ports": [
      2,
      {
        "quoteType": "double"
      }
    ]
  },
  "quiet": false,
  "debug": true,
  "exclude": [
    "tests"
  ]
}
