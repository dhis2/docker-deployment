{
  "rules": {
    "no-version-field": 0,
    "require-project-name-field": 0,
    "no-unbound-port-interfaces": [
      1,
      {
        "excludeServices": [
          "traefik"
        ]
      }
    ],
    "require-quotes-in-ports": [
      2,
      {
        "quoteType": "double"
      }
    ]
  },
  "quiet": false,
  "debug": true,
  "exclude": [
    "tests"
  ]
}
